{
  "name": "julienjovy/devx",
  "description": "Universal CLI tool to automate dev setup and maintenance (Laravel, Nuxt, Composer, ...)",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "bin": [
    "bin/devx"
  ],
  "require": {
    "php": "^8.3",
    "symfony/console": "^6.0 | ^7.0",
    "symfony/filesystem": "^6.0",
    "symfony/process": "^6.0 |^7.2",
    "guzzlehttp/guzzle": "^7.9"
  },
  "require-dev": {
    "pestphp/pest": "^2.36"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
